{
  "name": "fvas/lti-base",
  "type": "project",
  "description": "A base LTI module site for supporting LMS.",
  "keywords": ["tropotek", "tk004", "LTI"],
  "time": "2012-12-09",
  "license": "MIT",
  "version": "trunk",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Michael Mifsud",
      "email": "info@tropotek.com.au",
      "homepage": "http://www.tropotek.com/"
    }
  ],
  "autoload": {
    "psr-0": {
        "": [
          "src/",
          "plugin/"
        ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "config\\ComposerEvent::postInstall"
    ],
    "post-update-cmd": [
      "config\\ComposerEvent::postUpdate"
    ]
  },

  "require": {
    "php": ">=5.3.2",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-gd": "*",

    "fvas/tk-installers": "~1.0",

    "fvas/tk-domtemplate": "~1.0",
    "fvas/tk-framework": "~1.0",
    "fvas/tk-modlib": "~1.0",
    "fvas/tk-form": "~1.0",
    "fvas/tk-table": "~1.0",
    "fvas/tk-plugin": "~1.0",
    "fvas/tk-user": "~1.0",
    "fvas/tk-map": "~1.0",
    "fvas/tk-jslib": "~1.0",

    "oyejorge/less.php": "~1.5"
  },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
